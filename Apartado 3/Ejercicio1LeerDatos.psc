Algoritmo ejercicio1LeerDatos
	Definir num1, num2 Como Real;
	Escribir Sin Saltar "Introduzca el primer número: ";
	Leer num1;
	Escribir Sin Saltar "Introduzca el segundo número: ";
	Leer num2;
	Escribir Sin Saltar "El resultado de la multiplicación de los números introducidos es: ", (num1 * num2);
FinAlgoritmo
